  <div class='panel half-height'>
    <div class='panel-heading'>
      <h4>My Events</h4>
    </div>

    <div class='list'>
       <% if (events[1].length <= 0) { %>
         <div class='panel'>
           <p> You have not start any event yet. </p>
           <a href='/event/new'> Start Your own event now<a>
        </div>
       <% } else { %>
         <% events[1].forEach( (event)=> {%>
            <div class='col-md-12'>
              <p class='pull-left'>
                 <%= dateShow(event.startDate) %>
                 <strong><%= event.name %></strong>

                <% if (event.category !== null) {%>
                   <span class='label label-success'><%= event.category%></span>
                 <%}%>

                 <% if (dateCheck(event.startDate)) {%>
                   <span class='label label-danger'>Today</span>
                 <%}%>
              </p>
              <a class='pull-right' href='/event/<%= event._id%>/edit'>Edit</a>
           </div>
        <% }) %>
     <% } %>
    </div>
 </div>
